{
  "name": "radar-techno",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-toolkit-all": "^1.3.16",
    "@types/react-dom": "16.9.0",
    "luxon": "^1.22.0",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "mw.validation": "^1.0.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "react-enhanced-reducer-hook": "^1.0.2",
    "react-monaco-editor": "^0.34.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./scripts/inject-version.js && react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "ts": "./node_modules/.bin/tsc",
    "ts:watch": "./node_modules/.bin/tsc -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts",
      "pre-push": "npm test -- --coverage"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!/node_modules/",
      "!src/**/index.{ts,tsx}",
      "!src/serviceWorker.ts",
      "!src/typings.d.ts"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/addon-storyshots": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^4.2.5",
    "react-test-renderer": "^16.13.0",
    "require-context.macro": "^1.2.2",
    "storybook-react-router": "^1.0.8",
    "ts-jest": "^24.1.0",
    "typescript": "^3.8.3"
  }
}
