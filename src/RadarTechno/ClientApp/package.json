{
  "name": "radar-techno",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-toolkit-all": "^1.2.0",
    "@babel/polyfill": "^7.4.4",
    "@types/react-dom": "16.9.0",
    "luxon": "^1.21.3",
    "monaco-editor": "^0.18.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-sass": "^4.11.0",
    "mw.validation": "^1.0.15",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-enhanced-reducer-hook": "^1.0.2",
    "react-monaco-editor": "^0.30.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.5",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./scripts/inject-version.js && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "ts": "./node_modules/.bin/tsc",
    "ts:watch": "./node_modules/.bin/tsc -w",
    "tslint": "tslint src/**/*.ts{,x} --project tsconfig.json",
    "tslint:fix": "tslint src/**/*.ts{,x} --project tsconfig.json --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts && npm run tslint",
      "pre-push": "npm test -- --coverage"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!/node_modules/",
      "!src/**/index.{ts,tsx}",
      "!src/serviceWorker.ts",
      "!src/typings.d.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/addons": "^5.3.7",
    "@storybook/react": "^5.2.1",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.4",
    "react-test-renderer": "^16.8.5",
    "require-context.macro": "^1.0.4",
    "storybook-react-router": "^1.0.5",
    "ts-jest": "^24.1.0",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.3"
  }
}